    "vue-property-decorator": "^8.5.1",
    "vue-router": "^3.5.1",
    "vue-socket.io-extended": "^4.0.6",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-knobs": "^6.1.15",
